{
  "presets": [
    [ "@babel/env", {
        "targets": {
            "node": "current"
        },
        "modules": "commonjs"
    }],
    "@expressive/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
  ],
  "sourceMaps": false
}